/* Configure UART for console and AT command interface */
&uart0 {
	status = "okay";
	current-speed = <115200>;
};

/* Configure I2C for sensors (optional for future position tracking) */
&i2c2 {
	status = "okay";
};

/* Configure GPIO for status LEDs */
&gpio0 {
	status = "okay";
};
